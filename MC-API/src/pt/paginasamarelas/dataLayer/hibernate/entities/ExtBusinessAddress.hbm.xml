<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="pt.paginasamarelas.dataLayer.hibernate.entities">

    <class name="ExtBusinessAddress" table="SYSADM.ExtBusinessAddress">
        <id name="caNrid" column="ca0_nrid">
            <generator class="native"/>
        </id>
        <property name="city" column="city"/>
        <property name="line1" column="line1"/>
        <property name="line2" column="line2"/>
        <property name="postalCode" column="postal_code"/>
        <property name="region"  column="region"/>
        <property name="template" column="template"/>
        <property name="dmod" column="dmod"/>
    </class>

</hibernate-mapping>